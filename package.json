    "next": "^9.3.6",
    "next-fonts": "^1.4.0",
    "next-redux-wrapper": "^5.0.0",
    "next-routes": "^1.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-is": "^16.13.1",
    "react-redux": "^7.2.0",
