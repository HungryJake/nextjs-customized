    "helmet": "^3.22.0",
    "immutable": "^4.0.0-rc.12",
    "next": "^10.1.3",
    "next-fonts": "^1.4.0",
    "next-redux-wrapper": "^6.0.2",
    "next-routes": "^1.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-is": "^17.0.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
